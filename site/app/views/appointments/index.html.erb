<p id="notice"><%= notice %></p>
<h1 class="page-header text-center">Appointments</h1>
<div class="row">
  <div class="col-lg-10 col-lg-offset-1 calendar-container">


    <%= month_calendar events: @appointments do |date, appointments| %>
    <%= date %>

  <% appointments.each do |appointment| %>
    <div>
      <%= appointment.userName %>
    </div>
  <% end %>
  <% end %>
  <div class="text-center">
    <%= link_to 'New Appointment', new_appointment_path, {:class=>"btn btn-primary"} %>

  </div>
  </div>
</div>

<br>
